<?xml version="1.0" encoding="UTF-8"?> 
<beans 
 xmlns="http://www.springframework.org/schema/beans" 
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
 xmlns:p="http://www.springframework.org/schema/p" 
 xsi:schemaLocation="http://www.springframework.org/schema/beans 
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"> 
 
<bean id="employee123" class="com.mycompany.setter.Employee"> 
<property name="id" value="100"></property> 
<property name="name" value="Larry"></property>
<property name="salary" value="1000"></property>
<property name="project">
<map> 
<entry key="MIS" value="Amsterdam"></entry> 
<entry key="ERP" value="Chennai"></entry>
<entry key="BFI" value="Munich"></entry> 
</map> 
</property> 
</bean> 
 
</beans>